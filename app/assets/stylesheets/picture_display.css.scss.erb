/*plane picture*/
#pop_up{
	display:none;
	font-size:25px;
	color:black;
	position:fixed;
	bottom:12%;
	right:2%;
	text-shadow:10px 10px 10px red;
}

/*css for cronix show group photo*/
#group_name{
	cursor:pointer;
}
#group{
	display:none;
	
}

/*video css*/
#ad_video{
	cursor:pointer;
	font-size:45px;
	text-align:center;
	color:grey;
	text-shadow:10px 10px 10px lightblue;
	padding-bottom:3%;
	letter-spacing:5px;
}
#ad_video:hover{
		color:#615499;
		text-shadow:5px 5px 5px silver;
}
#video{
		display:none;
		width:100%;
		padding:5% 15%;
		padding-top:0;
}